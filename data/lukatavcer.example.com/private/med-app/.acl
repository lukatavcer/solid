@prefix  acl:  <http://www.w3.org/ns/auth/acl#>.

# Group authorization, giving Read/Write access to members of the Doctor group
<#authorization2>
    a               acl:Authorization;
    acl:accessTo    <./>;
    acl:mode        acl:Read,
                    acl:Write;
    acl:default <./>;
    acl:agentGroup  <https://example.com:8443/groups.ttl#Doctor>.