@prefix solid: <http://www.w3.org/ns/solid/terms#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix pim: <http://www.w3.org/ns/pim/space#>.
@prefix schema: <http://schema.org/>.
@prefix ldp: <http://www.w3.org/ns/ldp#>.
@prefix cert: <http://www.w3.org/ns/auth/cert#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<>
    a foaf:PersonalProfileDocument ;
    foaf:maker <https://lukatavcer.example.com:8443/profile/card#me> ;
    foaf:primaryTopic <https://lukatavcer.example.com:8443/profile/card#me> .

<https://lukatavcer.example.com:8443/profile/card#me>
    a foaf:Person ;
    a schema:Person ;

    foaf:name "Luka Tavcer Test" ;

    solid:account </> ;  # link to the account uri
    pim:storage </> ;    # root storage

    ldp:inbox </inbox/> ;

    pim:preferencesFile </settings/prefs.ttl> ;  # private settings/preferences
    solid:publicTypeIndex </settings/publicTypeIndex.ttl> ;
    solid:privateTypeIndex </settings/privateTypeIndex.ttl> ;

    cert:key [ a cert:RSAPublicKey;
    cert:modulus ""^^xsd:hexBinary;
    cert:exponent 65537 ] .
